<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Controller">
    <children>
        <TableView fx:id="tv_filelist" minHeight="-Infinity" minWidth="-Infinity" prefHeight="439.0" prefWidth="345.0">
        </TableView>
        <HBox alignment="CENTER" prefHeight="29.0" prefWidth="345.0">
            <children>
                <Button fx:id="bt_chooser_word" prefWidth="80.0" text="WORD">
                    <HBox.margin>
                        <Insets right="20.0" />
                    </HBox.margin>
                </Button>
            <Button fx:id="bt_chooser_pdf"  mnemonicParsing="false" prefWidth="80.0" text="PDF" />
                <Button fx:id="bt_extracter" prefWidth="80.0" text="解析">
                    <HBox.margin>
                        <Insets left="20.0" />
                    </HBox.margin>
                </Button>
            </children>
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
        </HBox>
    </children>
</VBox>
